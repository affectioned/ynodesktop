{
  "name": "ynodesktop",
  "version": "1.2.6",
  "description": "A Yume Nikki Online desktop client with optional Discord Rich Presence",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "start-nodrpc": "electron src/main-nodrpc.js",
    "dist": "electron-builder",
    "dist-nodrpc": "electron-builder -c.appId=jvbf.ynodesktop-nodrpc -c.productName=YNOdesktop-NoDRPC"
  },
  "author": "jvbf, abbey, foundationkitty",
  "license": "MIT",
  "devDependencies": {
    "electron": "^35.7.5",
    "electron-builder": "^25.1.8"
  },
  "build": {
    "appId": "jvbf.ynodesktop",
    "productName": "YNOdesktop",
    "artifactName": "${productName}-${version}-${os}-${arch}.${ext}",
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Game",
      "icon": "assets/logo.png"
    },
    "win": {
      "target": [
        "portable"
      ],
      "icon": "assets/logo.ico"
    }
  },
  "dependencies": {
    "@xhayper/discord-rpc": "^1.2.0",
    "electron-context-menu": "^3.6.1",
    "electron-store": "^8.2.0"
  }
}